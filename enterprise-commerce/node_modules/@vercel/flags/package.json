{
  "name": "@vercel/flags",
  "version": "2.2.1",
  "description": "Make the Vercel Toolbar aware of your feature flags and read overrides set by it",
  "keywords": [
    "feature flags",
    "Next.js",
    "react",
    "toolbar",
    "overrides"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs"
    },
    "./svelte": {
      "import": "./dist/svelte/index.js",
      "require": "./dist/svelte/index.cjs"
    },
    "./providers/launchdarkly": {
      "import": "./dist/providers/launchdarkly.js",
      "require": "./dist/providers/launchdarkly.cjs"
    },
    "./providers/split": {
      "import": "./dist/providers/split.js",
      "require": "./dist/providers/split.cjs"
    },
    "./providers/statsig": {
      "import": "./dist/providers/statsig.js",
      "require": "./dist/providers/statsig.cjs"
    },
    "./providers/optimizely": {
      "import": "./dist/providers/optimizely.js",
      "require": "./dist/providers/optimizely.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts",
        "dist/*.d.cts"
      ],
      "react": [
        "dist/react/index.d.ts",
        "dist/react/index.d.cts"
      ],
      "svelte": [
        "dist/svelte/index.d.ts",
        "dist/svelte/index.d.cts"
      ],
      "providers/*": [
        "dist/providers/*.d.ts",
        "dist/providers/*.d.cts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "jose": "5.2.1"
  },
  "devDependencies": {
    "@types/node": "20.11.17",
    "@types/react": "18.2.55",
    "@vitejs/plugin-react": "4.2.1",
    "msw": "2.1.7",
    "next": "14.1.0",
    "react": "18.2.0",
    "tsup": "8.0.1",
    "typescript": "5.3.3",
    "vite": "5.1.1",
    "vitest": "1.2.2",
    "eslint-config-custom": "0.0.0",
    "tsconfig": "0.0.0"
  },
  "peerDependencies": {
    "next": "*",
    "react": "*",
    "react-dom": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch --clean=false",
    "eslint": "eslint-runner",
    "eslint:fix": "eslint-runner --fix",
    "test": "vitest --run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  }
}